{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File", "TelegramWarnBot" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "TelegramSink",
        "Args": {
          "notifyBotOwners": [ 713766114 ],
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Data\\Logs.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "restrictedToMinimumLevel": "Error"
        }
      }
    ]
  }
}